{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ],
    "whitespace": false,
    "radix":false,
    "no-inferrable-types": false,
    "no-trailing-whitespace": false,
    "no-console": false
  }
}
